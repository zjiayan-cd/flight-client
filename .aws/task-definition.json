{
  "family": "jiayan-flight-client-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::381492153714:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "jiayan-flight-client-task-container",
      "image": "replace-this-later",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
